# DEP Framework - Пример файла переменных окружения
# Скопируйте этот файл в .env и заполните необходимые значения

# Настройки Solana
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com

# Ключи API
ETHERSCAN_API_KEY=your_etherscan_api_key_here
SOLSCAN_API_KEY=your_solscan_api_key_here

# Настройки базы данных
DB_HOST=localhost
DB_PORT=27017
DB_NAME=dep_framework
DB_USER=
DB_PASSWORD=

# Настройки NeuroSolidityAuditor
MODEL_PATH=./models/neuro_auditor_model
CONTRACTS_DB_PATH=./data/verified_contracts

# Настройки ProofOfHumanism
ETHICAL_THRESHOLD=75
DAO_CONTRACT_ADDRESS=your_dao_contract_address_here

# Настройки EthicalMirrorEngine
TRANSFORMATION_PATTERNS_PATH=./data/transformation_patterns.json
